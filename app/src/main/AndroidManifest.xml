<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tictac">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DefaultGameActivity" />
        <activity android:name=".Single_X_five" />
        <activity android:name=".Single_O_three" />
        <activity android:name=".Single_O_five" />
        <activity android:name=".TwoPlayer_three_by_three" />
        <activity android:name=".TwoPlayer_five_by_five" />
        <activity android:name=".single_x_four" />
        <activity android:name=".Single_O_four" />
        <activity android:name=".TwoPlayer_four_by_four" />
    </application>

</manifest>